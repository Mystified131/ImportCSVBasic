@model MVCApplication.ViewModels.ValuesViewModel

@{
    ViewData["Title"] = "Values";
}

<h2>Values</h2>

<h3>@Model.elements.Count Result(s)</h3>

<br />

@foreach (var element in Model.elements)
{
<table>

    <td>
    <tr>

        @foreach (KeyValuePair<string, string> item in element)
            {
        <p>@item.Key : @item.Value</p>
            }

    </tr>

    </td>

    <br />

</table>
}
